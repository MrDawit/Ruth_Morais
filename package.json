  "bugs": {
    "url": "https://github.com/MrDawit/Ruth_Morais/issues"
  },
  "homepage": "",
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "concurrently": "^7.2.2",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.23.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "googleapis": "^104.0.0",
    "if-env": "^1.0.4",
    "nodemailer": "^6.7.6",
    "nodemon": "^2.0.18",
    "run-all": "^1.0.1"
  },
  "devDependencies": {
